{
	"name": "laravel/laravel",
	"description": "The Laravel Framework.",
	"keywords": ["framework", "laravel"],
	"repositories": [
		{
			"type": "vcs",
			"url": "https://github.com/pixelgremlins/profiler.git"
		}
	],
	"require": {
		"laravel/framework": "4.0.*",
		"toddish/verify": "2.0.*",
		"laravel/curl": "dev-master",
		"cviebrock/eloquent-sluggable": "1.0.*",
		"gloudemans/shoppingcart": "dev-master",
		"juy/profiler" : "dev-master",
		"liebig/cron": "dev-master",
		"intervention/validation": "dev-master",
		"laravelbook/laravel4-powerpack": "dev-master",
		"ceesvanegmond/minify": "dev-master",
		"way/generators": "dev-master",
		"hailwood/smartless4laravel": "*",
		"laravelbook/ardent": "dev-master"

	},
	"autoload": {
		"classmap": [
			"app/commands",
			"app/controllers",
			"app/models",
			"app/database/migrations",
			"app/database/seeds",
			"app/tests/TestCase.php"
		]
	},
	"scripts": {
		"post-install-cmd": [
			"php artisan optimize"
		],
		"pre-update-cmd": [
			"php artisan clear-compiled"
		],
		"post-update-cmd": [
			"php artisan optimize"
		],
		"post-create-project-cmd": [
			"php artisan key:generate"
		]
	},
	"config": {
		"preferred-install": "dist"
	},
	"minimum-stability": "dev"
}
