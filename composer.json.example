{
	"name": "laravel/laravel",
	"description": "The Laravel Framework.",
	"keywords": ["framework", "laravel"],
	"repositories": [
		{
			"type": "vcs",
			"url": "https://github.com/pixelgremlins/profiler.git"
		}
	],
	"require": {
		"laravel/framework": "4.0.*",
		"cartalyst/sentry": "2.0.*",
		"cviebrock/eloquent-sluggable": "1.0.*",
		"gloudemans/shoppingcart": "dev-master",
		"juy/profiler" : "dev-master",
		"liebig/cron": "dev-master",
		"intervention/validation": "dev-master",
		"kmd/logviewer": "dev-master",
		"laravelbook/ardent": "dev-master",
		"guzzle/guzzle": "3.7.*@dev",
		"barryvdh/laravel-dompdf": " dev-master",
		"thujohn/analytics": "dev-master"

	},
	"autoload": {
		"classmap": [
			"app/commands",
			"app/controllers",
			"app/models",
			"app/database/migrations",
			"app/database/seeds",
			"app/tests/TestCase.php",
			"app/composers.php"
		],
	"files": [
		"lib/aws_signed_request.php",
		"lib/functions.php",
		"lib/simple_html_dom.php"
	]
	},
	"scripts": {
		"post-install-cmd": [
			"php artisan optimize"
		],
		"pre-update-cmd": [
			"php artisan clear-compiled"
		],
		"post-update-cmd": [
			"php artisan optimize"
		],
		"post-create-project-cmd": [
			"php artisan key:generate"
		]
	},
	"config": {
		"preferred-install": "dist"
	},
	"minimum-stability": "dev"
}
